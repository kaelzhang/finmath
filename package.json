{
  "name": "finmath",
  "version": "2.0.1",
  "description": "The FinTech utility collections of simple, cumulative, and exponential moving averages.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "babel src --out-dir lib",
    "test:no-cov": "ava --timeout=10s --verbose",
    "test": "NODE_DEBUG=dark-hole nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=dark-hole nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "posttest": "npm run report",
    "report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/finmath.git"
  },
  "keywords": [
    "finmath",
    "fintech",
    "math",
    "moving average",
    "moving-average",
    "moving-averages",
    "simple moving average",
    "exponential moving average",
    "weighted moving average",
    "smoothed moving average",
    "dynamic weighted moving average",
    "sma",
    "ema",
    "wma",
    "sma",
    "dma",
    "macd",
    "convergence",
    "divergence",
    "hhv",
    "llv",
    "highest",
    "lowest",
    "boll",
    "bollinger-bands",
    "array",
    "sub",
    "add"
  ],
  "ava": {
    "require": [
      "@babel/register"
    ],
    "testOptions": {
      "babelrc": true
    },
    "files": [
      "test/*.test.js"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/finmath/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@ostai/eslint-config": "^3.5.0",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "math-array": "^1.1.2"
  }
}
